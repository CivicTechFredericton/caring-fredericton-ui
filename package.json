    "classnames": "2.3.1",
    "env-cmd": "10.1.0",
    "formik": "2.2.8",
    "formik-material-ui": "3.0.1",
    "i18next": "20.3.1",
    "i18next-browser-languagedetector": "6.1.1",
    "i18next-xhr-backend": "3.2.2",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "node-sass": "4.14.1",
    "prop-types": "15.7.2",
    "query-string": "7.0.0",
    "react": "17.0.2",
    "react-big-calendar": "0.33.4",
