    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-config-prettier": "6.11.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.7",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "editor.formatOnSave": true,
    "proseWrap": "always",
