    "@material-ui/pickers": "3.2.10",
    "classnames": "2.2.6",
    "env-cmd": "10.1.0",
    "formik": "2.1.4",
    "formik-material-ui": "2.0.0-beta.1",
    "i18next": "19.4.5",
    "i18next-browser-languagedetector": "4.2.0",
    "i18next-xhr-backend": "3.2.2",
