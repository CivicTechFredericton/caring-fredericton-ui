{
  "name": "caring-calendar-ui",
  "version": "1.0.0",
  "description": "Caring Calendar React Project",
  "main": "src/index.jsx",
  "scripts": {
    "start": "webpack-dev-server -d --env.target dev --env.config local",
    "start:dev": "webpack-dev-server --env.target dev --env.config dev",
    "stats": "webpack --env.target analyze",
    "// prebuild:dev": "npm run test",
    "build:dev": "webpack --env.target build --env.config dev",
    "// prebuild:stage": "npm run test",
    "build:stage": "webpack --env.target build --env.config stage",
    "// prebuild:prod": "npm run test",
    "build:prod": "webpack --env.target build --env.config prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "storybook": "start-storybook -p 9001 .storybook"
  },
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.8"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(jpg|jpeg|gif|png|mp4|mkv|avi|webm|swf|wav|mid)$": "jest-static-stubs/$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "editor.formatOnSave": true,
    "proseWrap": "always",
    "requireConfig": false
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-knobs": "4.0.0",
    "@storybook/react": "4.0.0",
    "babel-7-jest": "21.3.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "compression": "1.7.3",
    "connect-history-api-fallback": "1.5.0",
    "css-loader": "1.0.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-matchers": "7.0.0",
    "es-abstract": "1.12.0",
    "eslint": "5.8.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-jest": "21.26.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-template": "6.2.0",
    "husky": "1.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-static-stubs": "0.0.1",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.4.4",
    "node-sass": "4.9.4",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "raf": "3.4.0",
    "react-hot-loader": "4.3.11",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.23.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4"
  },
  "dependencies": {
    "@babel/plugin-transform-object-assign": "7.0.0",
    "@babel/preset-stage-3": "7.0.0",
    "@material-ui/core": "3.3.2",
    "amazon-cognito-identity-js": "3.0.3",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-polyfill": "6.26.0",
    "class-autobind": "0.1.4",
    "classnames": "2.2.6",
    "connected-react-router": "4.5.0",
    "es6-promise": "4.2.5",
    "i18next": "12.0.0",
    "i18next-xhr-backend": "1.5.1",
    "immutable": "3.8.2",
    "moment": "2.22.2",
    "normalizr": "3.3.0",
    "react": "16.6.0",
    "react-big-calendar": "0.20.1",
    "react-dom": "16.6.0",
    "react-i18next": "8.3.5",
    "react-immutable-proptypes": "2.1.0",
    "react-immutable-render-mixin": "0.9.7",
    "react-redux": "5.1.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-actions": "2.6.3",
    "redux-form": "7.4.2",
    "redux-immutable": "4.0.0",
    "redux-normalizr-middleware": "2.0.0",
    "redux-saga": "0.16.2",
    "redux-saga-routines": "1.1.5",
    "regenerator-runtime": "0.12.1",
    "remove": "0.1.5",
    "reselect": "4.0.0",
    "roboto-fontface": "0.10.0",
    "spinkit": "1.2.5"
  }
}
